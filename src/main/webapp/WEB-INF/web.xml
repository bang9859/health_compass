<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:javaee="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xml="http://www.w3.org/XML/1998/namespace"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>ServiceServlet</servlet-name>
		<servlet-class>controller.ServiceServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServiceServlet</servlet-name>
		<url-pattern>/service/*</url-pattern>
	</servlet-mapping>
  
  
  	<servlet>
		<servlet-name>join</servlet-name>
		<jsp-file>/views/users/joinForm.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>join</servlet-name>
		<url-pattern>/join</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>login</servlet-name>
		<jsp-file>/views/users/loginForm.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>mypage</servlet-name>
		<jsp-file>/views/users/mypage.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>mypage</servlet-name>
		<url-pattern>/mypage</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>delete</servlet-name>
		<jsp-file>/views/users/deleteForm.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>delete</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>header</servlet-name>
		<jsp-file>/views/module/header.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>header</servlet-name>
		<url-pattern>/header</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>footer</servlet-name>
		<jsp-file>/views/module/footer.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>footer</servlet-name>
		<url-pattern>/footer</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>error</servlet-name>
		<jsp-file>/views/module/error.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>error</servlet-name>
		<url-pattern>/error</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>nav</servlet-name>
		<jsp-file>/views/module/nav.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>nav</servlet-name>
		<url-pattern>/nav</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>hospitalsForm</servlet-name>
		<jsp-file>/views/hospital/hospitalsForm.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>hospitalsForm</servlet-name>
		<url-pattern>/hospitals</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>detail</servlet-name>
		<jsp-file>/views/hospital/detail.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>detail</servlet-name>
		<url-pattern>/detail</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>main</servlet-name>
		<jsp-file>/views/hospital/main.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>main</servlet-name>
		<url-pattern>/main</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>schedule</servlet-name>
		<jsp-file>/views/schedule/schedule.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>schedule</servlet-name>
		<url-pattern>/schedule</url-pattern>
	</servlet-mapping>

	<error-page>
		<error-code>404</error-code>
		<location>/</location>
	</error-page>
</web-app>
